<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hamdisoap"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.hamdisoap.Act_Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hamdisoap.Act_Account"
            android:label="@string/title_activity_act__account"
            android:parentActivityName="com.example.hamdisoap.Act_Login" >
        </activity>
        <activity
            android:name="com.example.hamdisoap.Act_SingleFinder"
            android:label="@string/title_activity_act__single_finder"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar" >
        </activity>
        <activity
            android:name="com.example.hamdisoap.Act_SearchResult"
            android:label="@string/title_activity_act__search_result"
            android:parentActivityName="com.example.hamdisoap.Act_Login" >
        </activity>
    </application>

</manifest>